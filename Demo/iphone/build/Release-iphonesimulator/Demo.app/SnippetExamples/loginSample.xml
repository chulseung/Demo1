									<key>parameter</key>
									<dict>
										<key>message</key><string>??????????.</string>
									</dict>
								</dict>
							</dict>
							<key>SECURITY</key>
							<dict>
								<key>type</key><string>callAction</string>
								<key>name</key><string>ALERT</string>
								<key>extra</key>
								<dict>
									<key>parameter</key>
									<dict>
										<key>message</key><string>--{@record.responseData._state_}--</string>
									</dict>
								</dict>
							</dict>
						</dict>
					</array>
				</dict>
			</dict>
		</dict>
		<key>ALERT</key>
		<dict>
			<key>type</key><string>alert</string>
			<key>msg</key><string>@message</string>
