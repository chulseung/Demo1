							<key>extra</key>
							<dict>
								<key>parameter</key>
								<dict>
									<key>MAILID</key><string>@self.textfield1</string>
									<key>PWD</key><string>@self.textfield2</string>
								</dict>
							</dict>
							<key>SUCCESS</key>
							<dict>
								<key>saveAs</key><string>loginData</string>
